#include <stdio.h>

int main() {
    int arr[100]; 
    int n;

    printf("Enter the number of elements (maximum 100): ");
    scanf("%d", &n);

    if (n > 100 || n <= 0) {
        printf("Invalid input. Please enter a positive integer less than or equal to 100.\n");
        return 1; 
    }

    printf("Enter the elements:\n");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Contents of the integer array:\n");

    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    printf("\n");

    return 0;
}

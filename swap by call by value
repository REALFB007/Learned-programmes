#include <stdio.h>

void swapByValue(int a, int b) {
    int temp = a;
    a = b;
    b = temp;
}

int main() {
    int x = 5, y = 10;

    printf("Before swapping: x = %d, y = %d\n", x, y);

    swapByValue(x, y);

    printf("After swapping (Call by Value): x = %d, y = %d\n", x, y);

    return 0;
}
